{
  "current_project": "primary",
  "search_order": [
    "primary",
    "secondary"
  ],
  "projects": {
    "primary": {
      "url": "${SUPABASE_PRIMARY_URL}",
      "key": "${SUPABASE_PRIMARY_KEY}",
      "description": "Primary BTEB Results Database - Main production data"
    },
    "secondary": {
      "url": "${SUPABASE_SECONDARY_URL}",
      "key": "${SUPABASE_SECONDARY_KEY}",
      "description": "Secondary BTEB Results Database"
    }
  },
  "settings": {
    "enable_fallback": true,
    "max_projects_to_search": 2,
    "timeout_per_project": 10,
    "log_search_attempts": true,
    "cache_results": false
  }
}